          p!=CALLTOACTIOTEXT
                             
          a.btn.btn-default.btn-xl.wow.tada(type='button', data-toggle='modal', data-target='#exampleModal', data-whatever='consultation')!=SIGNUP2


block faq 
    section#contact
      .container
        .row
          .col-lg-8.col-lg-offset-2.text-center
            h2.section-heading!=CONTACTUS
            |                     
            hr.primary
            p!=CONTACTUSNOTE
          .col-lg-4.col-md-4.text-center
            i.fa.fa-phone.fa-3x.wow.bounceIn
            p!=PHONE
          .col-lg-4.col-md-4.text-center
            i.fa.fa-envelope-o.fa-3x.wow.bounceIn(data-wow-delay='.1s')
            p
              a(href='mailto:!=EMAIL')!=EMAIL
          .col-lg-4.col-md-4.text-center
            i.fa.fa-3x.wow.bounceIn(data-wow-delay='.1s')="ЮНА"
            p
              a(href="http://juna-life.ru/" target="_blank")!=SITE
