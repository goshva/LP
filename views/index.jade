      .header-content
        .header-content-inner
          h1!=H1
          hr
          p.slogan!=SLOGAN
          a.btn.btn-primary.btn-xl.page-scroll(href='#', data-toggle='modal', data-target='#exampleModal', data-whatever='callMe')!=CALLME
block boomcall     
    section#about.boom.bg-primary
      .container
        .row
          .col-lg-8.col-lg-offset-2.text-center
            p.text-faded!=BOOMNOTE1
            a.btn.btn-default.btn-xl.wow.tada(type='button', data-toggle='modal', data-target='#exampleModal', data-whatever='consultation')!=SIGNUP0

block boomcalculate     
    section#phase.boom.bg-primary
      .container
        .row
          .col-lg-8.col-lg-offset-2.text-center
            p.text-faded!=BOOMNOTE2
            a.btn.btn-default.btn-xl.wow.tada(type='button', data-toggle='modal', data-target='#exampleModal', data-whatever='calculation')!=SIGNUP1
block boom     
    section#about.bg-primary
      .container
        .row
          .col-lg-8.col-lg-offset-2.text-center
            h2.section-heading!=BOOM
            hr.light
              
            h3.text-faded!=BOOMNOTE3
            a.btn.btn-default.btn-xl.wow.tada(type='button', data-toggle='modal', data-target='#exampleModal', data-whatever='coupon')!=BOOMBUTTON
block arguments     
    section#about
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading!=WHYUS
            |                     
            hr.primary
      .container
        .row
         - var n = 0
            while n < 8
              .col-lg-3.col-md-4.col-sm-6.col-xs-6.text-center.advantages
                .service-box
                 - var classfafa = ICONSADVANTAGES[(n)]
                  img(src='img/pictograms/'+(n+1)+'.png', alt='',width='100')
                  h4!=ADVANTAGES[(n)]
                  //p.text-muted!=NAMESADVANTAGES[(n)] 
                - n++
block phase     
    section
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading!=PHASESNOTE
            |                     
            hr.primary
      |         
      .container
        .row
         - var n = 0
            while n < 8
              .col-lg-3.col-md-4.col-sm-6.col-xs-6.text-center.phases
                .service-box
                  img(src='img/phase/'+(n+1)+'.png', alt='PHASENOTE[(n)')
                  h3!=PHASES[(n)]
                  p.text-muted!=PHASENOTE[(n)]
              - n++
block portfolio   
    section#portfolio
      .container-fluid
        .row.no-gutter
          .col-lg-12.text-center
            h2.section-heading!=EXAMPLES
            hr.primary
          - var n = 0
            while n < 7
              .col-lg-4.col-md-6
                span.portfolio-box
                  img.img-responsive(src='img/areas/'+(n+1)+'.jpg', alt='')
                  .portfolio-box-caption
                    .portfolio-box-caption-content
                        .project-category.text-faded!=PORTAREAS[(n)]
                        .project-name!=PORTAREANAMES[(n)]
              - n++
block portfolio2  
    section#services
      .container-fluid
        .row.no-gutter
          .col-lg-12.text-center
            h2.section-heading!=SERVICES
            hr.primary
          - var n = 0
            while n < 9
              .col-lg-4.col-sm-6
                span.portfolio-box(href="")
                  img.img-responsive(src='img/services/'+(n+1)+'.jpg', alt='')
                  .portfolio-box-caption
                    .portfolio-box-caption-content
                      .project-category.text-faded!=PORTCATEGORYES[(n)]
              - n++	
block trust
    aside.bg-dark
      .container.text-center
        .call-to-action
